{
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "8",
          "chrome": "59",
          "safari": "10",
          "firefox": "56",
          "edge": "14"
        },
        "modules": "commonjs"
      },
      "@babel/preset-typescript"
    ]
  ],
  "plugins": ["lodash", "@babel/plugin-proposal-class-properties", "babel-plugin-styled-components"]
}
